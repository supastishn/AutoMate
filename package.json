{
  "name": "automate",
  "version": "0.1.0",
  "description": "AutoMate - Fast personal AI assistant. Self-hosted, multi-channel, tool-using agent.",
  "type": "module",
  "bin": {
    "automate": "./dist/index.js"
  },
  "scripts": {
    "dev": "node --import tsx src/index.ts",
    "build": "tsup src/index.ts --format esm --target node22 --clean --dts",
    "start": "node dist/index.js",
    "gateway": "node --import tsx src/index.ts gateway",
    "gateway:watch": "node --import tsx --watch src/index.ts gateway",
    "ui:dev": "cd ui && npm run dev",
    "ui:build": "cd ui && npm install && npm run build"
  },
  "dependencies": {
    "fastify": "^5.2.1",
    "@fastify/websocket": "^11.0.1",
    "@fastify/static": "^8.0.3",
    "@fastify/cors": "^10.0.2",
    "discord.js": "^14.16.3",
    "selenium-webdriver": "^4.27.0",
    "zod": "^3.24.1",
    "commander": "^13.1.0",
    "chalk": "^5.4.1",
    "nanoid": "^5.0.9"
  },
  "devDependencies": {
    "tsx": "^4.19.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "@types/node": "^22.10.5"
  },
  "engines": {
    "node": ">=22"
  }
}
